<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="arrayCollection" class="spring.di.AnimalCollection">
		<property name="animalList">
			<list>
				<value>Tiger</value>
				<value>Lion</value>
				<value>Tiger</value>
			</list>
		</property>
		<property name="animalMap">
			<map>
				<entry key="1" value="Tiger" />
				<entry key="2" value="Lion" />
				<entry key="3" value="Tiger" />
			</map>
		</property>
		<property name="animalSet">
			<set>
				<value>Lion</value>
				<value>Tiger</value>
				<value>Lion</value>
			</set>
		</property>
		<property name="animalPro">
			<props>
				<prop key="one">Lion</prop>
				<prop key="two">Tiger</prop>
				<prop key="three">Lion</prop>
			</props>
		</property>
	</bean>
</beans>
